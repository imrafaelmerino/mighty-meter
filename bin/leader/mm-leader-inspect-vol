#!/usr/bin/env bash
set -o errexit
set -o pipefail
set -o nounset
# set -o xtrace
echo ""
echo "Executing script:$0 $@"
echo ""

main(){

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

source "${DIR}"/../.defaults

docker volume inspect \
${_NAME_VOLUME_GRAFANA} \
${_NAME_VOLUME_INFLUXDB} \
${_NAME_VOLUME_JMETER_LOGS} \
${_NAME_VOLUME_JMETER_REPORTS} \
${_NAME_VOLUME_JMETER_CONF}


}

main "$@"