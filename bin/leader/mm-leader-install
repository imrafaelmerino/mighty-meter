#!/usr/bin/env bash
set -o errexit


echo ""
echo "Executing script: $0 $@"
echo ""

main(){
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

if ! $DIR/../lib/mm-leader-build; then
   exit 1
fi

if ! $DIR/../lib/mm-leader-run; then
   exit 1
fi

if ! $DIR/../lib/mm-leader-init-metrics-db; then
   exit 1
fi

if ! $DIR/../lib/mm-leader-init-dashboards; then
   exit 1
fi

}

main "$@"