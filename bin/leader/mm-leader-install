#!/usr/bin/env bash
set -o errexit
set -o pipefail
set -o nounset
# set -o xtrace
echo ""
echo "Executing script:$0 $@"
echo ""

main(){
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

source ../.versions
source ../.defaults
source ../lib/functions

../lib/mm-leader-build

../lib/mm-leader-run

../lib/mm-leader-init-metrics-db

../lib/mm-leader-init-dashboards

}

main "$@"