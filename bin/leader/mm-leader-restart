#!/usr/bin/env bash

echo ""
echo "Executing script: $0 $@"
echo ""

main() {
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source "${DIR}"/../.defaults

docker restart ${_NAME_LEADER_GRAFANA_CONTAINER}
docker restart ${_NAME_LEADER_INFLUXDB_CONTAINER}
docker restart ${_NAME_LEADER_NGINX_CONTAINER}
}

main "$@"
