#!/usr/bin/env bash


echo ""
echo "(MIGHTY-METER-INSTALLER) | Executing script: $0 $@"
echo ""

main() {

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source "${DIR}"/../.defaults

docker stop ${_NAME_LEADER_GRAFANA_CONTAINER}
docker stop ${_NAME_LEADER_INFLUXDB_CONTAINER}
docker stop ${_NAME_LEADER_NGINX_CONTAINER}

}

main "$@"