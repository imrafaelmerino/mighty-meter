#!/usr/bin/env bash
set -o errexit


echo ""
echo "(MIGHTY-METER-INSTALLER) | Executing script: $0 $@"
echo ""

main(){
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

source "${DIR}"/../.versions
source "${DIR}"/../.defaults
source "${DIR}"/../lib/functions

docker exec -it ${_NAME_LEADER_JMETER_START_TESTS_CONTAINER} stoptest.sh 4445

}

main "$@"