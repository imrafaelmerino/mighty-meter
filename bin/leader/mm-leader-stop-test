#!/usr/bin/env bash
set -o errexit
set -o pipefail
set -o nounset
# set -o xtrace
echo ""
echo "Executing script:$0 $@"
echo ""

main(){
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source ../.versions
source ../.defaults
source ../lib/functions

docker exec -it ${_NAME_LEADER_JMETER_START_TESTS_CONTAINER} stoptest.sh 4445

}

main "$@"