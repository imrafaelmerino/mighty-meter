#!/usr/bin/env bash
set +e
assertNotEmpty () {
  if [[ -z $2 ]]
    then
      echo "(MIGHTY-METER-INSTALLER) | $1"
      exit 1
  fi
}

# Error handler function
error_exit() {
    echo "(MIGHTY-METER-INSTALLER) | ERROR | $1" >&2
    exit 1
}

print_message() {
   echo "(MIGHTY-METER-INSTALLER) | $1"
}

print_command() {
   echo "(MIGHTY-METER) | $1"
}

print_uninstaller_message() {
   echo "(MIGHTY-METER-UNINSTALLER) | $1"
}





